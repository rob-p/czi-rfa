	Mesh = {Calibration; Chromosomes, Human, Y; Computational Biology; DNA Mutational Analysis; DNA, Mitochondrial; Evolution, Molecular; Female; Genetic Association Studies; Genetic Variation; Genetics, Population; Genome, Human; Genome-Wide Association Study; Genomics; Genotype; Haplotypes; Humans; Male; Mutation; Pilot Projects; Polymorphism, Single Nucleotide; Recombination, Genetic; Sample Size; Selection, Genetic; Sequence Alignment; Sequence Analysis, DNA},
