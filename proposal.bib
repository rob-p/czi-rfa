	Journal = {Nucleic Acids Res},
	Journal-Full = {Nucleic acids research},
	Mesh = {Algorithms; Alternative Splicing; Arabidopsis; DNA, Complementary; Expressed Sequence Tags; Genome, Plant; Introns; Plant Proteins; RNA, Plant; Sequence Alignment; Software; Transcription, Genetic; Untranslated Regions},
	Month = {Oct},
	Number = {19},
	Pages = {5654-66},
